<?xml version="1.0" encoding="UTF-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            x:Class="PrismBug.Pages.TabbedPage"
            xmlns:pages="clr-namespace:PrismBug.Pages" >
    <TabbedPage.Children>
        <NavigationPage Title="Page A" >
            <x:Arguments>
                <pages:PageA />
            </x:Arguments>
        </NavigationPage>

        <NavigationPage Title="Page B" >
            <x:Arguments>
                <pages:PageB />
            </x:Arguments>
        </NavigationPage>
        </TabbedPage.Children>
</TabbedPage>
